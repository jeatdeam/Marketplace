                div(class="marca marcaTwo", style="background-image:url('/img/numbuzinBanner.png')", data-Id="2", data-name="numbuzin") N
                div(class="marca marcaThree", style="background-image:url('/img/skin1004Banner.png')", data-id="3", data-name="skini1004") S
                div(class="marca marcaFour", style="background-image:url('/img/anuaBanner.png')", data-id="4", data-name="anua") A
                div(class="marca marcaFive", style="background-image:url('/img/bojBanner.png')", data-id="5", data-name="boj") B
