        section.masVendidos
            h1 PRODUCTOS MAS VENDIDOS
            div.movimientoMasVendidos
                div.containerMasVendidos_one
                    if baseDatos && baseDatos.length > 0
                        each element,indice in baseDatos
                          div(class=`masVendidoElement masVendidoElement_${indice+1}`, data-categoria=element.category, data-brand=element.brand, data-name=element.name )
                               img(src=element.img[0])
                               b #{element.subtitle}
                               small S/.#{element.price}
                               div
                                    button(data-id=element.id, data-brand=element.brand, data-img=element.img[0], data-price=element.price, data-name=element.name) agregar al carrito
                                    button(data-id=element.id, data-brand=element.brand, data-img=element.img[0], data-price=element.price, data-name=element.name) comprar
                    else
                        p No se encontraron productos
                div.containerMasVendidos_two
                    if baseDatos && baseDatos.length > 0
                        each element,indice in baseDatos
                            div(class=`masVendidoElement masVendidoElement_${indice + 1}`, data-categoria=element.category, data-brand=element.brand, data-name=element.name)
